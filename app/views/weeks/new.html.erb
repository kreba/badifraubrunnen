<% content_for :title, @title = t('weeks.new.title') %>


<%= render( 'shared/error_messages', obj: @week ) %>

<%=t 'weeks.new.taken_numbers' %>: <%= Week.all.collect(&:number).sort.join(', ') %>

<%= form_with model: @week, local: true do |f| %>
  <p>
    <b><%=t 'weeks.new.week_number_label' %></b><br />
    <%= f.text_field :number %>
  </p>

  <p>
    <%= f.submit t('Save') %>
  </p>
<% end %>
