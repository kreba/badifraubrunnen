<% content_for :admin_links do %><li><%= link_to( t('weeks.new.title'), new_week_path )%></li><% end %>

<%= render( :partial => 'week_jumper', :locals => { :week => @week } )%>

<% for @saison in @saisons %>

  <% if @saisons.size > 1 %>
    <h2><%=t "saisons.#{@saison.name}" %></h2>
  <% end %>

  <%= render( :partial => 'week_plan' )%>
  <%= link_to( t('weeks.show.printout'), 
    { :action => "render_week_plan", :id => @week, :saison_id => @saison }, :popup => true )%>
  <h2><br /></h2>

<% end %>

